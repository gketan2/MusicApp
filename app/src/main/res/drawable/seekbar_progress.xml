<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <clip>
            <shape android:shape="rectangle">
                <size android:height="3dp" />
                <gradient
                    android:type="linear"
                    android:angle="0"
                    android:startColor="?attr/seekbarProgressStartColor"
                    android:endColor="?attr/seekbarProgressEndColor"
                    />
            </shape>
        </clip>
    </item>
</layer-list>
